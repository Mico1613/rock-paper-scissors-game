{
  "name": "rps-game",
  "version": "0.0.1",
  "description": "Just a simple rps game",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "code-check": "pnpm run prettier-check && pnpm run eslint-check",
    "prettier-check": "prettier --check src/",
    "prettier-write": "prettier --write src/",
    "eslint-check": "eslint --ext .tsx,.ts src/ --max-warnings=0",
    "eslint-fix": "eslint --ext .tsx,.ts src/ --fix",
    "prepare": "husky install"
  },
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-solid": "^0.12.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "normalize.css": "^8.0.1",
    "prettier": "^2.8.7",
    "sass": "^1.60.0",
    "stylelint": "^15.4.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-solid": "^2.5.0",
    "vite-plugin-solid-svg": "^0.6.1"
  },
  "dependencies": {
    "solid-js": "^1.6.10"
  }
}
